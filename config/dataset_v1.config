params {
  // Debug related, limiter = n ,
  // limits list runs first n items,  set to -1 for production, will run with all,
  limiter = 4

   scop_version = 2.05
   pdb_seqres_url = 'http://ftp.wwpdb.org/pub/pdb/derived_data/pdb_seqres.txt.gz'

   // Used for mmseqs clustering
   min_seq_id = 0.98
