process {
    cpus = 1
    executor = 'slurm'
    maxForks = 28
    time = '30m'
    errorStrategy = 'ignore'

    withLabel: time_limit_2h {
     time = '2h'
    }


    withLabel: med_job {
      executor = 'slurm'
      maxForks = 56
      cpus = 2
    }

    withLabel: big_job {
      cpus = 1
      maxForks = 8
      executor = 'slurm'
    }

}
